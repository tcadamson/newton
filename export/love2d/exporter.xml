<exporter>
    <name>love2d</name>
    <displayName>Love2D</displayName>
    <description>Exporter for the LOVE framework</description>
    <version>1.0</version>
    <yAxisDirection>down</yAxisDirection>
    <physicsEngine>box2d</physicsEngine>
    <template>love2d.lua</template>
    <fileExtension>lua</fileExtension>
    <anchorPoint>
        <enabled>no</enabled>
    </anchorPoint>
    <origin>
        <type>fixed</type>
        <relX>0.0</relX>
        <relY>1.0</relY>
    </origin>
    <global>
    </global>
    <body>
        <parameter>
            <name>static</name>
            <displayName>Static</displayName>
            <type>bool</type>
            <default>false</default>
        </parameter>
        <parameter>
            <name>fixed_rotation</name>
            <displayName>Fixed Rotation</displayName>
            <type>bool</type>
            <default>false</default>
        </parameter>
        <parameter>
            <name>linear_damping</name>
            <displayName>Linear Damping</displayName>
            <type>float</type>
            <min>0.0</min>
            <max>5.0</max>
            <default>0.5</default>
        </parameter>
        <parameter>
            <name>angular_damping</name>
            <displayName>Angular Damping</displayName>
            <type>float</type>
            <min>0.0</min>
            <max>5.0</max>
            <default>0.5</default>
        </parameter>
    </body>
    <fixture>
        <parameter>
            <name>sensor</name>
            <displayName>Sensor</displayName>
            <type>bool</type>
            <default>false</default>
        </parameter>
        <parameter>
            <name>density</name>
            <displayName>Density</displayName>
            <type>float</type>
            <min>0.1</min>
            <max>100.0</max>
            <default>1.0</default>
        </parameter>
        <parameter>
            <name>friction</name>
            <displayName>Friction</displayName>
            <type>float</type>
            <min>0.0</min>
            <max>100.0</max>
            <default>0.0</default>
        </parameter>
        <parameter>
            <name>restitution</name>
            <displayName>Restitution</displayName>
            <type>float</type>
            <min>0.0</min>
            <max>1.0</max>
            <default>0.0</default>
        </parameter>
        <parameter>
            <name>filter_group</name>
            <displayName>Group</displayName>
            <shortDescription>Fixtures in the same non-zero group do not collide</shortDescription>
            <type>uint16</type>
            <default>0</default>
        </parameter>
        <parameter>
            <name>filter_bitfield</name>
            <type>bitfield</type>
            <size>16</size>
        </parameter>
        <parameter>
            <name>filter_category</name>
            <displayName>Cat</displayName>
            <type>uint16</type>
            <default>1</default>
            <bitfield>yes</bitfield>
        </parameter>
        <parameter>
            <name>filter_mask</name>
            <displayName>Mask</displayName>
            <type>uint16</type>
            <default>65535</default>
            <bitfield>yes</bitfield>
        </parameter>
    </fixture>
</exporter>